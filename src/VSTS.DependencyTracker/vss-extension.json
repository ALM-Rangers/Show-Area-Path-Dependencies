{
  "manifestVersion": 1,
  "id": "ShowAreaPathDependencies",
  "version": "0.0.0",
  "name": "Show Area Path Dependencies",
  "description": "Show area path dependencies a team has with other teams.",
  "publisher": "",
  "public": true,
  "scopes": [
    "vso.work",
    "vso.profile"
  ],
  "icons": {
    "default": "static/images/default-icon.png"
  },
  "tags": [
    "Work",
    "Dependency",
    "Backlog",
    "Team"
  ],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Plan and track"
  ],
  "content": {
    "details": {
      "path": "marketplace/readme.md"
    },
    "license": {
      "path": "marketplace/mp_terms.md"
    }
  },
  "links": {
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    },
    "issues": {
      "uri": "https://github.com/ALM-Rangers/Show-Area-Path-Dependencies-Extension/issues"
    },
    "repository": {
      "uri": "https://github.com/ALM-Rangers/Show-Area-Path-Dependencies-Extension"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ALM-Rangers/Show-Area-Path-Dependencies-Extension"
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "static/dist/src/dependency-module/Configuration.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/Configuration.js"
    },
    {
      "path": "static/dist/src/dependency-module/ConfigurationDialog.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/ConfigurationDialog.js"
    },
    {
      "path": "static/dist/src/dependency-module/DataService.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/DataService.js"
    },
    {
      "path": "static/dist/src/dependency-module/DependencyTracker.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/DependencyTracker.js"
    },
    {
      "path": "static/dist/src/dependency-module/Helpers.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/Helpers.js"
    },
    {
      "path": "static/dist/src/dependency-module/infrastructure.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/infrastructure.js"
    },
    {
      "path": "static/dist/src/dependency-module/TelemetryClient.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/TelemetryClient.js"
    },
    {
      "path": "static/dist/src/dependency-module/UIService.js",
      "addressable": true,
      "packagePath": "dist/src/dependency-module/UIService.js"
    },
    {
      "path": "static/css",
      "addressable": true,
      "packagePath": "/css"
    },
    {
      "path": "static/images",
      "addressable": true,
      "packagePath": "/images"
    },
    {
      "path": "static/lib",
      "addressable": true,
      "packagePath": "/lib"
    },
    {
      "path": "static/index.html",
      "addressable": true,
      "packagePath": "index.html"
    },
    {
      "path": "static/select_columns.html",
      "addressable": true,
      "packagePath": "select_columns.html"
    }
  ],

 "contributions": [
    {
      "id": "dependency-tracker-hub",
      "type": "ms.vss-web.hub",
      "description": "Shows dependencies on the teams backlog",
      "targets": [
        "ms.vss-work-web.work-hub-group"
      ],
      "properties": {
        "name": "Dependencies",
        "icon": "images/configure.png",
        "order": 100,
        "uri": "index.html"
      }
    },
    {
      "id": "dependency-tracker-hub-tab",
      "type": "ms.vss-web.tab",
      "description": "Shows dependencies on the teams backlog",
      "targets": [
        "ms.vss-work-web.product-backlog-tabs"
      ],
      "properties": {
        "uri": "index.html",
        "title": "Dependencies",
        "name": "Dependencies",
        "registeredObjectId": "showDependencies"
      }
    },
    {
      "id": "configuration",
      "type": "ms.vss-web.control",
      "description": "Configuration Options",
      "targets": [],
      "properties": {
        "uri": "select_columns.html"
      }
    }
  ]
}
