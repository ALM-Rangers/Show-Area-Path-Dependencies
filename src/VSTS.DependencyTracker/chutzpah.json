{
  "Framework": "jasmine",
  "TestFileTimeout": 60000,
  "TestHarnessReferenceMode": "AMD",
  "TypeScriptModuleKind": "AMD",
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "References": [
    { "Path": "./node_modules/vss-web-extension-sdk/lib/vss.sdk.js" },
    { "Path": "./node_modules/requirejs/require.js" },
    { "Path": "./node_modules/jquery/dist/jquery.js" },
    { "Path": "./node_modules/jasmine/bin/jasmine.js" },
    {
      "Includes": [ "*/src/dependency-module/*.ts" ],
      "Excludes": [ "*/src/*.d.ts" ]
    }
  ],
  "Tests": [
    {
      "Excludes": [ "*.d.ts", "specrunner.ts" ],
      "Includes": [ "*/test/*.ts" ]
    }
  ]
}